{
  "name": "ngx-ng2-semantic-ui",
  "version": "16.0.0",
  "description": "Angular Semantic UI Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikita-fuchs/ng2-semantic-ui"
  },
  "bugs": {
    "url": "https://github.com/nikita-fuchs/ng2-semantic-ui/issues"
  },
  "author": "https://github.com/nikita-fuchs/ng2-semantic-ui/graphs/contributors",
  "keywords": [
    "angular",
    "angular-components",
    "semantic-ui",
    "no-jquery",
    "ui",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "lib:lint": "ng lint lib",
    "lib:test": "ng test lib --code-coverage --source-map false --progress false --watch false",
    "lib:compile:locales": "tsc --project src/behaviors/localization/tsconfig.locales.json && node ./scripts/locales",
    "lib:compile": "ng build lib && npm run lib:compile:locales",
    "toEditor": "npm run lib:compile && cd dist && npm pack && mv *.tgz /Users/ethereumbook/Documents/DAPPS/Angular/Angular8/demo-form-sku/src/libs",
    "toEditorLive": "npm run lib:compile && cd dist && npm pack && mv *.tgz /Users/ethereumbook/Documents/DAPPS/Angular/Angular8/demo-form-sku/src/libs && cd /Users/ethereumbook/Documents/DAPPS/Angular/Angular8/demo-form-sku/ && npm install && ng serve",
    "lib:compile:w": "ng build lib --watch",
    "lib:compile:prod": "ng build lib --configuration production && npm run lib:compile:locales",
    "lib:build": "npm run lib:lint && npm run lib:compile",
    "lib:build:prod": "npm run lib:lint && npm run lib:compile:prod",
    "demo:serve": "ng serve demo --host 0.0.0.0",
    "demo:lint": "ng lint demo",
    "demo:compile": "ng build demo",
    "demo:compile:prod": "ng build demo --configuration production",
    "demo:build": "npm run demo:compile",
    "demo:build:prod": "npm run demo:compile:prod",
    "lint": "npm run lib:lint && npm run demo:lint",
    "test": "npm run lib:test",
    "build": "npm run lib:build && npm run demo:build",
    "build:prod": "npm run lib:build:prod && npm run demo:build:prod"
  },
